	position: relative;
	margin: 0;
	padding: 0;
	list-style: none;
}
.olapic .olapic-carousel li {
	position: relative;
	width: 140px;
	height: 140px;
	margin: 0 7.5px;
	float: left;
	overflow: hidden;
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
}
.olapic .olapic-carousel li .olapic-user-name {
	position: relative;
	z-index: 2;
	max-width: 140px;
	padding: 0 10px;
	display: none;
	overflow: hidden;
	font-family: Arial, Helvetica, sans-serif; 
	font-size: 15px;
	font-weight: normal;
	line-height: 140px;
	text-align: center;
	white-space: nowrap;
	text-overflow: ellipsis;
	color: #fff;
	background-color: rgba(0, 0, 0, .6);
}
.olapic .olapic-carousel li.olapic-partial-item {
	display: block;
	border: 2px solid #dcd6ca;
	background: 
url('//olapic-data.s3.amazonaws.com/toms/images/olapic-toms-empty-logo.png')
 center center no-repeat;
}
.olapic .olapic-carousel li a:link,
.olapic .olapic-carousel li a:visited {
	width: 100%; 
	height: 100%;
	display: block;
    background-size: cover;
}
.olapic .olapic-carousel li a:link .olapic-user-name,
.olapic .olapic-carousel li a:visited .olapic-user-name {
	position: relative;
	z-index: 999999;
	max-width: 100%;
	padding: 0 10px;
	display: none;
	overflow: hidden;
	font-size: 15px;
	font-weight: normal;
	line-height: 140px;
	text-align: center; 
